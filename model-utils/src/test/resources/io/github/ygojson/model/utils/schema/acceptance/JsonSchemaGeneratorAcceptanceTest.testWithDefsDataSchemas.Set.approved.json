{
  "$schema" : "https://json-schema.org/draft/2019-09/schema",
  "$defs" : {
    "SetInfo" : {
      "type" : "object",
      "properties" : {
        "name" : {
          "type" : "string"
        },
        "nameAlt" : {
          "type" : "string"
        },
        "setCode" : {
          "type" : "string",
          "pattern" : "[a-zA-Z0-9]+"
        }
      }
    },
    "SetLocalizedData" : {
      "type" : "object",
      "properties" : {
        "de" : {
          "$ref" : "#/$defs/SetInfo"
        },
        "es" : {
          "$ref" : "#/$defs/SetInfo"
        },
        "fr" : {
          "$ref" : "#/$defs/SetInfo"
        },
        "it" : {
          "$ref" : "#/$defs/SetInfo"
        },
        "ja" : {
          "$ref" : "#/$defs/SetInfo"
        },
        "ko" : {
          "$ref" : "#/$defs/SetInfo"
        },
        "pt" : {
          "$ref" : "#/$defs/SetInfo"
        },
        "zh-Hans" : {
          "$ref" : "#/$defs/SetInfo"
        },
        "zh-Hant" : {
          "$ref" : "#/$defs/SetInfo"
        }
      }
    }
  },
  "type" : "object",
  "properties" : {
    "id" : {
      "type" : "string",
      "format" : "uuid"
    },
    "name" : {
      "type" : "string"
    },
    "nameAlt" : {
      "type" : "string"
    },
    "setCode" : {
      "type" : "string",
      "pattern" : "[a-zA-Z0-9]+"
    },
    "type" : {
      "type" : "string",
      "pattern" : "[a-z-0-9]+"
    },
    "series" : {
      "type" : "string",
      "pattern" : "[a-z-0-9]+"
    },
    "localizedData" : {
      "$ref" : "#/$defs/SetLocalizedData"
    }
  },
  "required" : [ "id" ],
  "$id" : "io.github.ygojson.model.data.Set"
}