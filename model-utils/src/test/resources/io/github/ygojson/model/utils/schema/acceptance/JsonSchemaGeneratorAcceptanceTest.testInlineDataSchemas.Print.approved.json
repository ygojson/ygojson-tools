{
  "$schema" : "https://json-schema.org/draft/2019-09/schema",
  "type" : "object",
  "properties" : {
    "id" : {
      "type" : "string",
      "format" : "uuid"
    },
    "cardId" : {
      "type" : "string",
      "format" : "uuid"
    },
    "setId" : {
      "type" : "string",
      "format" : "uuid"
    },
    "printCode" : {
      "type" : "string",
      "pattern" : "[a-zA-Z0-9]+-[a-zA-Z0-9]+"
    },
    "rarity" : {
      "type" : "string",
      "pattern" : "[a-z]+"
    },
    "language" : {
      "type" : "string",
      "enum" : [ "en", "de", "es", "fr", "it", "ja", "ko", "pt", "zh-Hans", "zh-Hant" ]
    }
  },
  "required" : [ "id", "cardId", "setId" ],
  "$id" : "io.github.ygojson.model.data.Print"
}