{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "definitions" : {
    "CardLocalizedData" : {
      "type" : "object",
      "properties" : {
        "de" : {
          "$ref" : "#/definitions/CardText"
        },
        "es" : {
          "$ref" : "#/definitions/CardText"
        },
        "fr" : {
          "$ref" : "#/definitions/CardText"
        },
        "it" : {
          "$ref" : "#/definitions/CardText"
        },
        "ja" : {
          "$ref" : "#/definitions/CardText"
        },
        "ko" : {
          "$ref" : "#/definitions/CardText"
        },
        "pt" : {
          "$ref" : "#/definitions/CardText"
        },
        "zh-Hans" : {
          "$ref" : "#/definitions/CardText"
        },
        "zh-Hant" : {
          "$ref" : "#/definitions/CardText"
        }
      }
    },
    "CardText" : {
      "type" : "object",
      "properties" : {
        "name" : {
          "type" : "string"
        },
        "effectText" : {
          "type" : "string"
        },
        "flavorText" : {
          "type" : "string"
        },
        "materials" : {
          "type" : "string"
        },
        "pendulumEffect" : {
          "type" : "string"
        }
      }
    },
    "CardType" : {
      "type" : "string",
      "enum" : [ "monster", "spell", "trap" ]
    },
    "Identifiers" : {
      "type" : "object",
      "properties" : {
        "konamiId" : {
          "type" : "integer"
        },
        "password" : {
          "type" : "integer"
        },
        "passwordAlt" : {
          "type" : "integer"
        },
        "yugipediaPageId" : {
          "type" : "integer"
        }
      }
    },
    "LinkArrow" : {
      "type" : "string",
      "enum" : [ "top-left", "top-center", "top-right", "middle-left", "middle-right", "bottom-left", "bottom-center", "bottom-right" ]
    }
  },
  "type" : "object",
  "properties" : {
    "id" : {
      "type" : "string",
      "format" : "uuid"
    },
    "identifiers" : {
      "$ref" : "#/definitions/Identifiers"
    },
    "name" : {
      "type" : "string"
    },
    "cardType" : {
      "$ref" : "#/definitions/CardType"
    },
    "property" : {
      "type" : "string",
      "pattern" : "[a-z]+"
    },
    "effectText" : {
      "type" : "string"
    },
    "monsterTypes" : {
      "type" : "array",
      "items" : {
        "type" : "string"
      }
    },
    "flavorText" : {
      "type" : "string"
    },
    "attribute" : {
      "type" : "string",
      "pattern" : "[a-z]+"
    },
    "atk" : {
      "type" : "integer",
      "minimum" : 0
    },
    "atkUndefined" : {
      "type" : "boolean"
    },
    "def" : {
      "type" : "integer",
      "minimum" : 0
    },
    "defUndefined" : {
      "type" : "boolean"
    },
    "level" : {
      "type" : "integer",
      "minimum" : 0
    },
    "materials" : {
      "type" : "string"
    },
    "pendulumEffect" : {
      "type" : "string"
    },
    "pendulumScale" : {
      "type" : "integer",
      "minimum" : 0
    },
    "linkRating" : {
      "type" : "integer",
      "minimum" : 0
    },
    "linkArrows" : {
      "maxItems" : 8,
      "type" : "array",
      "items" : {
        "$ref" : "#/definitions/LinkArrow"
      }
    },
    "xyzRank" : {
      "type" : "integer",
      "minimum" : 0
    },
    "localizedData" : {
      "$ref" : "#/definitions/CardLocalizedData"
    }
  },
  "required" : [ "id" ],
  "$id" : "io.github.ygojson.model.data.Card"
}